[color]
  ui = true
[color "branch"]
  current = blue bold
  local = yellow
  remote = magenta
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  whitespace = white reverse
  commit = yellow bold
[color "status"]
  added = green bold
  changed = blue bold
  untracked = magenta bold
  deleted = red bold
  branch = yellow bold
[alias]
  st = status
  di = diff
  ci = commit
  br = branch
  llog = log --date=local
  pretty = log --graph --pretty=format:'%C(bold yellow)%h%Creset -%C(bold magenta) %s %C(bold cyan)(%cr) %C(bold green)<%an>%Creset' --abbrev-commit --date=relative
[rerere]
  enabled = true
[push]
	default = simple
